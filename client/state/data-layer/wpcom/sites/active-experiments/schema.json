{
	"$schema": "http://json-schema.org/draft-04/schema#",
	"type": "object",
	"required": [ "experiments", "ttl" ],
	"properties": {
		"experiments": {
			"type": "array",
			"items": {
				"type": "object",
				"required": [ "name", "variation" ],
				"properties": {
					"name": {
						"type": "string"
					},
					"variation": {
						"type": "string"
					}
				}
			}
		},
		"ttl": {
			"type": "integer"
		}
	}
}
